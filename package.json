{
  "name": "naive-ui-pro",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build": "vite build -c vite.build.ts",
    "build:lint": "vue-tsc && vite build -c vite.build.ts",
    "lint": "eslint . --fix --ext .ts,.tsx,.vue,.js,.jsx",
    "fix": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "format": "prettier --write \"./**/*.{js,ts,tsx,jsx,json,css,scss}\""
  },
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./esm/index.d.ts",
      "import": "./esm/index.js",
      "require": "./lib/index.js"
    },
    "./es": {
      "types": "./esm/index.d.ts",
      "import": "./esm/index.js"
    },
    "./lib": {
      "types": "./dist/index.d.ts",
      "require": "./lib/index.js"
    },
    "./es/*.js": {
      "types": "./esm/*.d.ts",
      "import": "./esm/*.js"
    },
    "./es/*": {
      "types": [
        "./esm/*.d.ts",
        "./esm/*/index.d.ts"
      ],
      "import": "./es/*.mjs"
    },
    "./lib/*.js": {
      "types": "./lib/*.d.ts",
      "require": "./lib/*.js"
    },
    "./lib/*": {
      "types": [
        "./lib/*.d.ts",
        "./lib/*/index.d.ts"
      ],
      "require": "./lib/*.js"
    },
    "./package.json": "./package.json",
    "./*": "./*"
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "dependencies": {
    "vue": "^3.3.11"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.20.1",
    "naive-ui": "^2.37.3",
    "prettier": "3.2.4",
    "typescript": "^5.3.3",
    "vfonts": "^0.0.3",
    "vite": "^5.0.8",
    "vite-plugin-dts": "^3.7.1",
    "vue-tsc": "^1.8.25"
  }
}
